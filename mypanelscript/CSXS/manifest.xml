<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="6.0" ExtensionBundleId="com.mustthank.panel-script" ExtensionBundleVersion="2.1.2"
    ExtensionBundleName="Mustthank Panel Script" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Author>Marshall Jones</Author>
    <Abstract><![CDATA[Mustthank Panel Script]]></Abstract>
  <ExtensionList>
    <Extension Id="com.mustthank.panel-script" Version="2.1.2" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="19.0" />
      <Host Name="PHXS" Version="15.0" />
      <Host Name="PHSP" Version="15.0" />
      <Host Name="AEFT" Version="13.0" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="6.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.mustthank.panel-script">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <ScriptPath>./jsx/hostscript.jsx</ScriptPath>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>My Panel Script</Menu>
          <Geometry>
            <Size>
              <Height>300</Height>
              <Width>800</Width>
            </Size>
            <MaxSize>
              <Height>1000</Height>
              <Width>2000</Width>
            </MaxSize>
            <MinSize>
              <Height>300</Height>
              <Width>200</Width>
            </MinSize>
             
          </Geometry>
          <Icons> 
          <Icon Type="Normal">./images/icon.png</Icon> 
          <Icon Type="RollOver">./images/hover.png</Icon>
          <Icon Type="DarkNormal">./images/icon.png</Icon> 
          <Icon Type="DarkRollOver">./images/hover.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>